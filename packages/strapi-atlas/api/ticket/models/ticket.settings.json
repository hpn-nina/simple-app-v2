{
  "kind": "collectionType",
  "collectionName": "tickets",
  "info": {
    "name": "Ticket"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "ticketTitle": {
      "type": "string",
      "required": true
    },
    "desc": {
      "type": "richtext",
      "required": true
    },
    "dateCreated": {
      "type": "date"
    },
    "from_user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "tickets"
    },
    "images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    }
  }
}
